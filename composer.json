{
    "name": "mdantas/express-request",
    "description": "ExpressRequest plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": ">3.2",
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "ExpressRequest\\": "src/"
        },
        "files": [
            "src/functions.php"
        ],
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ExpressRequest\\Test\\": "tests/"
        }
    },
    "scripts": {
        "tests": "phpunit tests"
    }
}
